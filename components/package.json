{
	"name": "@directus/website-components",
	"version": "0.0.1",
	"description": "Self-Referential Responsive Components",
	"homepage": "https://directus.io",
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/directus/website.git",
		"directory": "components"
	},
	"license": "MIT",
	"author": "Rijk van Zanten <rijkvanzanten@me.com>",
	"type": "module",
	"exports": {
		".": "./dist/index.js",
		"./theme": "./src/theme/main.css",
		"./style": "./dist/style.css",
		"./nuxt": "./nuxt.ts",
		"./package.json": "./package.json"
	},
	"main": "dist/index.js",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "vite build",
		"dev": "vite build --watch",
		"test": "vitest --watch=false",
		"story:dev": "histoire dev",
		"story:build": "histoire build",
		"story:preview": "histoire preview",
		"docs": " vue-docgen '**/*[!story].vue' -c docgen.config.cjs",
		"typecheck": "vue-tsc --noEmit"
	},
	"devDependencies": {
		"@histoire/plugin-vue": "0.16.1",
		"@histoire/shared": "0.16.1",
		"@vitejs/plugin-vue": "4.2.3",
		"@vitest/coverage-c8": "0.33.0",
		"@vue/tsconfig": "0.4.0",
		"histoire": "0.16.2",
		"rollup-plugin-node-externals": "6.1.1",
		"typescript": "5.1.6",
		"vite": "4.4.4",
		"vite-plugin-dts": "3.3.1",
		"vitest": "0.33.0",
		"vue-docgen-cli": "4.67.0",
		"vue-tsc": "1.8.5"
	},
	"dependencies": {
		"@nuxt/kit": "3.6.3",
		"pinia": "2.1.4",
		"vue": "3.3.4",
		"vue-i18n": "9.2.2",
		"vue-router": "4.2.4"
	}
}
